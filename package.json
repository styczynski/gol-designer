    "uglifyify": "^3.0.2",
    "opener": "*"
  },
  "scripts": {
    "build": "grunt --gruntfile ./bin/build.coffee --base ./ build --stack",
    "build-d": "grunt --gruntfile ./bin/build.coffee --base ./ build -d -v --stack",
    "release": "grunt --gruntfile ./bin/build.coffee --base ./ release --stack",
    "publish": "grunt --gruntfile ./bin/build.coffee --base ./ publish --stack",
    "server": "grunt --gruntfile ./bin/build.coffee --base ./ server --stack",
    "clean": "grunt --gruntfile ./bin/build.coffee --base ./ clean --stack",
    "help": "grunt --gruntfile ./bin/build.coffee --base ./ help --no-color",
    "show": "grunt --gruntfile ./bin/build.coffee --base ./ show --no-color",
    "send": "git add . & git commit -a -m \"Next release.\" & git push"
  },
  "license": "MIT",
  "coffeelintConfig": {
    "max_line_length": {
      "level": "ignore",
      "value": 0
    },
    "no_backticks": {
      "level": "warn"
    }
  }
}
